{
  "name": "daicss",
  "version": "0.11.3",
  "description": "Librería de estilos base del Sistema de Diseño para DAI Conacyt",
  "main": "src/dai.scss",
  "scripts": {
    "start": "concurrently \"npm run scss\" \"npm run docs\" ",
    "dist": "sass src/dai.scss:dist/dai.min.css --style compressed",
    "build:docs": "cd docs && npm run build",
    "docs": "cd docs && npm run dev",
    "install:docs": "cd docs && npm install",
    "scss": "sass --watch src/dai.scss:docs/src/assets/dai.css --style expanded"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conacyt-dai/daicss.git"
  },
  "author": "flkt.crnpio@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conacyt-dai/daicss/issues"
  },
  "homepage": "https://github.com/conacyt-dai/daicss#readme",
  "dependencies": {
    "sass": "1.49.10"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "devDependencies": {
    "concurrently": "7.2.1"
  }
}
